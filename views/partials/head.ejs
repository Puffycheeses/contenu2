<meta charset="UTF-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<meta http-equiv="X-UA-Compatible" content="ie=edge">
<title>Contenu</title>

<link rel="stylesheet" href="https://raw.githack.com/privatesuite/psm.css/master/css/app.css">
<script src="https://raw.githack.com/privatesuite/psm.css/master/js/app.js"></script>

<script>

function getCookie (name) {
	
	const parts = ("; " + document.cookie).split("; " + name + "=");
	if (parts.length == 2) return parts.pop().split(";").shift();

}

</script>

<style>

	.app {

		width: 100%;
		height: 100%;

	}

	#menu {

		position: fixed;

		top: 0;
		left: 0;

		padding: 20px;

		width: 10%;
		min-width: 175px;
		max-width: 500px;

		overflow: hidden;

	}

	#menu>* {

		display: inline-block;

		margin-bottom: 10px;

		border-left: 0px solid #1B1B1B;
		border-radius: 0;

		padding: 5px 10px;

		width: 100%;

		font-size: 18pt;
		font-weight: 500;
		text-decoration: none;

		transition: 0.2s all;

	}

	#menu>*:hover {

		border-left: 5px solid #1B1B1B;
		border-radius: 5px;

	}

	.main_section {

		margin-left: 175px;

		width: calc(100% - 175px);

	}

	.half_split>* {

		width: 50%;

	}

	.half_split>*:last-child {

		margin-left: 20px;

	}

	.third_split>* {

		width: 33%;

	}

	.third_split>*:nth-child(2), .third_split>*:last-child {

		margin-left: 20px;

	}

</style>
